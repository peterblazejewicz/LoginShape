<?xml version="1.0" encoding="UTF-8"?>
<?xaml-comp compile="true" ?>

<ResourceDictionary
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Color x:Key="DarkBlack">#222222</Color>
    <Color x:Key="LightBlack">#f1f1f1</Color>

    <SolidColorBrush x:Key="DarkBackground" Color="{StaticResource DarkBlack}" />
    <SolidColorBrush x:Key="LightBackground" Color="{StaticResource LightBlack}" />
</ResourceDictionary>